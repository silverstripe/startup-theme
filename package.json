{
    "name": "silverstripe-startup-theme",
    "version": "1.0.0",
    "private": true,
    "engines": {
        "node": "^20.x"
    },
    "scripts": {
        "dev": "npx mix",
        "watch": "npx mix watch",
        "build": "cross-env NODE_ENV=production npx mix --production",
        "prettier:check": "prettier --check src/**/*.{scss,vue}",
        "prettier:write": "prettier --write src/**/*.{scss,vue}",
        "lint:css": "stylelint src/scss/**/*.scss src/js/**/*.vue",
        "lint:css:fix": "stylelint --fix src/scss/**/*.scss src/js/**/*.vue",
        "lint:js": "eslint --ext .js,.vue src/js",
        "lint": "yarn lint:css && yarn lint:js"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/silverstripe/startup-theme"
    },
    "license": "BSD-3-Clause",
    "dependencies": {
        "path": "^0.12.7",
        "vue": "^3.2.47"
    },
    "devDependencies": {
        "@babel/core": "^7.21.3",
        "@vue/compiler-sfc": "^3.2.47",
        "babel-loader": "^9.1.2",
        "babel-plugin-istanbul": "^6.0.0",
        "babel-plugin-syntax-dynamic-import": "^6.18.0",
        "browser-sync": "^2.29.1",
        "browser-sync-webpack-plugin": "^2.3.0",
        "clean-webpack-plugin": "^4.0.0",
        "cross-env": "^7.0.3",
        "dotenv": "^16.0.3",
        "esbuild": "^0.19.3",
        "eslint": "^8.37.0",
        "eslint-config-airbnb-base": "^15.0.0",
        "eslint-config-prettier": "^8.8.0",
        "eslint-plugin-import": "^2.27.5",
        "eslint-plugin-vue": "^9.10.0",
        "eslint-webpack-plugin": "^4.0.0",
        "fs-extra": "^11.1.1",
        "html-webpack-plugin": "^5.5.0",
        "laravel-mix": "^6.0.49",
        "modern-normalize": "^2.0.0",
        "node-sass-glob-importer": "^5.3.2",
        "postcss": "^8.4.31",
        "postcss-html": "^1.5.0",
        "postcss-scss": "^4.0.6",
        "prettier": "^2.8.7",
        "resolve-url-loader": "^5.0.0",
        "sass": "^1.60.0",
        "sass-loader": "^13.2.2",
        "stylelint": "^15.3.0",
        "stylelint-config-prettier": "^9.0.5",
        "stylelint-config-recommended-vue": "^1.4.0",
        "stylelint-config-standard": "^28.0.0",
        "stylelint-scss": "^4.6.0",
        "stylelint-webpack-plugin": "^4.1.0",
        "vue-loader": "^17.0.1",
        "webpack": "^5.74.0"
    }
